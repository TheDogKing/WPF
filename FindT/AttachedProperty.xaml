<Window x:Class="FindT.AttachedProperty"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FindT"
    mc:Ignorable="d"
    Title="OK" Height="200" Width="350">
    <Canvas>
        <Button Name="Button"  Width="80" Canvas.Top="100" Content="1" Canvas.Left="100" local:TestAttachedProperty.Angle="40" />
        <TextBox Name="TextBox" Width="100" Background="Black" Canvas.Top="20" Canvas.Right="20"/>
    </Canvas>
</Window>
